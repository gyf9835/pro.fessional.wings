  <template name="wgmp" value="/**&#10; * $Aa$ to $Bb$ mapper, auto generate by `wgmp` live template&#10; */&#10;@org.mapstruct.Mapper&#10;public interface $A2B$ {&#10;&#10;    $A2B$ INSTANCE = org.mapstruct.factory.Mappers.getMapper($A2B$.class);&#10;&#10;    /**&#10;     * create new $Bb$ by $Aa$&#10;     *&#10;     * @param a $Aa$&#10;     * @return $Bb$&#10;     */&#10;    static $B$ into($A$ a) {&#10;        return INSTANCE._into(a);&#10;    }&#10;&#10;    /**&#10;     * build $Bb$ with $Aa$&#10;     *&#10;     * @param a $Aa$&#10;     * @param b $Bb$&#10;     */&#10;    static void into($A$ a, $B$ b) {&#10;        INSTANCE._into(a, b);&#10;    }&#10;&#10;    $M$&#10;    $B$ _into($A$ a);&#10;&#10;    $M$&#10;    void _into($A$ a, @org.mapstruct.MappingTarget $B$ b);&#10;}" description="mapstruct Create &amp; Update" toReformat="true" toShortenFQNames="true">
    <variable name="A" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="B" expression="" defaultValue="" alwaysStopAt="true" />
