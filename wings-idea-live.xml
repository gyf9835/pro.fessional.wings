    <variable name="Aa" expression="regularExpression(A,&quot;&lt;.*&gt;|.*\\.&quot;,&quot;&quot;)" defaultValue="" alwaysStopAt="false" />
    <variable name="A2B" expression="concat(Aa,&quot;To&quot;,Bb)" defaultValue="" alwaysStopAt="false" />
